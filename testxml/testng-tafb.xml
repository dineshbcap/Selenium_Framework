<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Selenium Automation Framework">
    
    <listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
  </listeners>       
	
    <test name="com.projectname.test.functional.modulename">
    
     <groups>  
		   <define name="Regression Tests">    
		      <include name="Regression"/>           
		    </define>
		    <run>
		    	 <include name="Regression Tests"/>
		    </run>
	</groups>  
  
  
    <packages>
      <package name="com.projectname.test.functional.modulename1.*"/>
    </packages>
	</test>
	
	 <test name="com.projectname.test.functional.modulename1">
    <packages>
      <package name="com.projectname.test.functional.modulename2.*"/>
    </packages>
	</test>
	
	 <test name="com.projectname.test.functional.trainingsystem.*">
    <packages>
      <package name="com.projectname.test.functional.trainingsystem.*"/>
    </packages>
	</test>
	
</suite> <!-- [EBS Selenium Automated QA Test]] -->