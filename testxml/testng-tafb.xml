<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Selenium Automation Framework">
    
    <listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
  </listeners>       
	
    <test name="com.projectname.test.functional.login" parallel="methods" thread-count="3">
    
     <groups>  
		   <define name="Regression Tests">    
		      <include name="Regression"/>           
		    </define>
		    <run>
		    	 <include name="Regression Tests"/>
		    </run>
	</groups>  
  
  
    <packages>
      <package name="com.projectname.test.functional.login.*"/>
    </packages>
	</test>
	
	 <test name="com.projectname.test.functional.idm" parallel="methods" thread-count="3">
    <packages>
      <package name="com.projectname.test.functional.idm.*"/>
    </packages>
	</test>
	
	 <test name="com.projectname.test.functional.trainingsystem" parallel="methods" thread-count="3">
    <packages>
      <package name="com.projectname.test.functional.trainingsystem.*"/>
    </packages>
	</test>
	<test name="com.projectname.test.functional.searchusers" parallel="methods" thread-count="3">
    <packages>
      <package name="com.projectname.test.functional.searchusers.*"/>
    </packages>
	</test>
	
</suite> <!-- [EBS Selenium Automated QA Test]] -->